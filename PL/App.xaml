<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             xmlns:conv="clr-namespace:PL.Converters"
             StartupUri="Login/LoginWindow.xaml">
    <Application.Resources>
        <conv:StatusToColorConverter x:Key="StatusToColorConverter" />
        <conv:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
        <local:CallTypeCollection x:Key="CallTypeCollection" />
        <local:CallStatusCollection x:Key="CallStatusCollection" />
        <local:VolunteerSortFieldCollection x:Key="VolunteerSortFieldCollection" />
    </Application.Resources>
</Application>